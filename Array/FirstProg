// Simple array program to print entered number

package Array;

public class FirstProg {
    public static void main(String[] args){
        int[] num = new int[5]; // declare + initialize
        num[0]=5;
        num[1]=10;
        num[2]=15;
        num[3]=20;
        num[4]=25;
        for(int i=0;i<5;i++) {
            System.out.println(num[i]);
        }
    }
}
